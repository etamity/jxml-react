---
version: 1.0

render:
  Div_App:
    className: grid grid-cols-2 gap-2
    Div_code:
      Editor:
        height: 100vh
        defaultLanguage: yaml
        value: this.props.code
        theme: vs-dark
        onChange: this.props.onCodeChange
        options:
          tabSize: 2
          wordWrap: true
    Div_preview:
      className: bg-white p-2 rounded border
      RuntimePreview: this.props.preview
