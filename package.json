    "@mdx-js/runtime": "^1.6.22",
    "@monaco-editor/react": "^4.1.3",
    "@popperjs/core": "2.9.1",
    "@rematch/core": "^2.0.1",
    "@rematch/immer": "^2.0.1",
    "@rematch/loading": "^2.0.1",
    "@webpack-cli/generators": "^2.2.0",
    "antd": "^4.16.2",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.0.1",
    "connected-react-router": "^6.9.1",
