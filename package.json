{
  "name": "jxml-react",
  "version": "1.0.12",
  "homepage": "https://etamity.github.io/jxml-react",
  "description": "Render react view from Yaml file",
  "repository": "etamity/jxml-react",
  "main": "./dist/jxml-react.js",
  "module": "./dist/jxml-react.esm.js",
  "umd:main": "./dist/jxml-react.umd.js",
  "source": "src/index.js",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "react",
    "yaml",
    "yml",
    "json",
    "components",
    "ui"
  ],
  "exports": {
    "./loader": "./src/loader/index.js",
    ".": "./dist/jxml-react.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "microbundle --define process.env.NODE_ENV=production --jsx React.createElement --output dist",
    "serve": "webpack serve --config docSrc/webpack.config.js",
    "build:docs": "webpack --mode=production --node-env=production --config docSrc/webpack.config.js",
    "build:modules": "webpack --config docSrc/webpack.module.config.js --mode=production --node-env=production",
    "publish:bundle": "npm run build && npm version patch && npm publish"
  },
  "author": "Joey.Etamity",
  "license": "MIT",
